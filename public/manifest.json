{
  "manifest_version": 3,
  "name": "PC Cleaner",
  "version": "1.0",
  "description": "Clear browser history, cookies, and cache",
  "permissions": ["browsingData"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
